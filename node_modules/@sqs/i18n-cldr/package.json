{
  "name": "@sqs/i18n-cldr",
  "version": "2.3.1",
  "description": "SQSP cldr packs and config",
  "license": "UNLICENSED",
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "packs/"
  ],
  "homepage": "https://github.com/sqsp/i18n-frontend-tools/tree/master/libraries/i18n-cldr",
  "repository": {
    "type": "git",
    "url": "git@github.com:sqsp/i18n-frontend-tools.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.squarespace.net/artifactory/api/npm/npm-all/"
  },
  "bugs": {
    "url": "https://squarespace.atlassian.net/jira/software/c/projects/I18N/issues/",
    "email": "i18n-eng@squarespace.com"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@phensley/cldr-core": "~1.8.3",
    "@phensley/cldr-compiler": "~1.8.3",
    "typescript": "^5.2.2",
    "prettier": "^3.1.1",
    "simple-git": "^3.26.0",
    "@sqs/i18n-locale-list": "^2.3.0",
    "@sqs/prettier-config": "^1.1.1"
  },
  "scripts": {
    "lint:fix": "prettier --write ./src/index.ts ./src/config.json",
    "build": "npm run build:pack && npm run build:lib",
    "build:lib": "tsc",
    "build:pack": "cldr-compiler pack -o ./packs -c ./src/config.json",
    "clean": "rm -rf packs",
    "packdiff": "npm run build:pack && node scripts/packdiff/index.mjs"
  }
}