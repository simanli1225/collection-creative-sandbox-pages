{
  "name": "@sqs/enums",
  "version": "2.244.0",
  "description": "Squarespace enums for javascript",
  "main": "throw Error('Enums modules should be required individually')",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://artifactory.squarespace.net:443/artifactory/api/npm/npm-all/"
  },
  "scripts": {
    "lint": "eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx src/",
    "deleteEnums": "rm -rf src/*.ts",
    "makeDocs": "scripts/makeDocs.js",
    "clean": "rimraf .rush lib && scripts/clean.js",
    "build": "febs build"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@sqs/babel-preset-febs": "^15.0.1",
    "@sqs/eslint-config-typescript": "^3.0.2",
    "@sqs/eslint-plugin": "^3.0.1",
    "@sqs/febs-cli": "^15.0.1",
    "@sqs/febs-ts-config": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-plugin-transform-typescript-enums": "^0.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.1",
    "glob": "^7.1.6",
    "rimraf": "^2.6.2",
    "typescript": "^4.8.4",
    "core-js": "^3.0.0"
  },
  "keywords": [
    "squarespace",
    "enums",
    "javascript"
  ],
  "author": "Daniel Duan <dduan@squarespace.com>",
  "license": "UNLICENSED"
}
