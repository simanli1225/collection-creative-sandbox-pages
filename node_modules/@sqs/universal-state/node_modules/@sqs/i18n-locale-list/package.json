{
  "name": "@sqs/i18n-locale-list",
  "version": "2.3.0",
  "description": "A frontend package to serve as a source of truth for the locales we support at Squarespace.",
  "homepage": "https://github.com/sqsp/i18n-frontend-tools/tree/master/libraries/i18n-locale-list",
  "repository": {
    "type": "git",
    "url": "git@github.com:sqsp/i18n-frontend-tools.git"
  },
  "keywords": [
    "locales",
    "internationalization"
  ],
  "author": "i18n-eng@squarespace.com",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://artifactory.squarespace.net/artifactory/api/npm/npm-all/"
  },
  "bugs": {
    "url": "https://squarespace.atlassian.net/jira/software/c/projects/I18NPLAT/boards/1394/",
    "email": "i18n-eng@squarespace.com"
  },
  "engines": {
    "npm": ">=6.2",
    "node": ">=14"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "cjs",
    "lib"
  ],
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "testPathIgnorePatterns": [
      "fixtures",
      ".d.ts"
    ],
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  },
  "peerDependencies": {
    "@babel/runtime": "^7.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@jest/globals": "^29.0.0",
    "@sqs/babel-preset-febs": "^21.0.0",
    "@sqs/browserslist-config-squarespace": "^1.4.0",
    "@sqs/eslint-config-typescript": "^3.1.1",
    "@sqs/eslint-plugin": "^3.0.1",
    "@sqs/febs-cli": "^21.0.0",
    "@sqs/febs-ts-config": "^21.0.0",
    "@sqs/prettier-config": "^1.1.1",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "core-js": "^3.0.0"
  },
  "browserslist": [
    "extends @sqs/browserslist-config-squarespace"
  ],
  "scripts": {
    "build": "npm run clean && febs build",
    "clean": "rimraf lib cjs",
    "lint": "eslint  --max-warnings=0 --ext .ts src/",
    "lint:fix": "eslint --max-warnings=0 --ext .ts --fix src",
    "test": "jest",
    "type-check": "tsc"
  }
}