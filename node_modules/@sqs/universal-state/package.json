    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "febs build --watch --log-level debug",
    "release:start": "build-scripts/release-start",
    "release:commit": "build-scripts/release-commit $npm_package_version",
    "release:push": "build-scripts/release-push $npm_package_version",
    "preversion": "./build-scripts/npm-pre-version",
    "postversion": "./build-scripts/npm-post-version $npm_package_version",
    "sonar-scan": "sonar-scanner -Dsonar.host.url=${SONAR_HOST} -Dsonar.login=${SONAR_TOKEN}",
    "version": "./build-scripts/npm-version $npm_package_version"
  },
  "dependencies": {
    "@babel/runtime": "7.18.9",
