{
  "name": "@sqs/network",
  "version": "9.2.1",
  "description": "Native-React Network Bridge",
  "keywords": [
    "network",
    "bridge",
    "react-native",
    "ios",
    "android"
  ],
  "homepage": "https://github.com/sqsp/network-bridge",
  "bugs": {
    "url": "https://github.com/sqsp/network-bridge/issues"
  },
  "license": "UNLICENSED",
  "author": "The Squarespace Mobile Team <mobile-eng@squarespace.com>",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "react-native": "src/index.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/sqsp/network-bridge.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./utils": {
      "types": "./lib/utils.d.ts",
      "default": "./lib/utils.js"
    }
  },
  "scripts": {
    "build": "babel src --out-dir lib --delete-dir-on-start --copy-files --ignore '**/*.native.js','**/__mocks__/','**/__tests__/'",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "eslint ./src --fix",
    "prepare": "husky install",
    "test": "f() { jest \"$@\" && jest --config jest.native.config.js \"$@\"; }; f",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "commitlint": "commitlint --edit",
    "git:commit": "git-cz --disable-emoji"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@sqs/babel-preset-febs": "^17.1.0",
    "@sqs/commitlint-cli": "^7.5.4",
    "@sqs/commitlint-config-conventional": "^7.5.4",
    "@sqs/eslint-config-squarespace": "^6.0.0",
    "@sqs/eslint-plugin-squarespace": "^1.1.0",
    "@sqs/react-native-toolkit": "^6.3.6",
    "axios-mock-adapter": "^1.21.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "react": "17.0.2",
    "react-native": "0.67.5",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "react-native": "^0.67.5 || ^0.68.7",
    "@sqs/react-native-toolkit": "^6.2.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "@sqs/react-native-toolkit": {
      "optional": true
    }
  },
  "dependencies": {
    "@sqs/cookie-cutter": "^0.3.6",
    "axios": "^0.21.1",
    "core-js": "^3.0.0",
    "qs": "^6.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.squarespace.net:443/artifactory/api/npm/npm-all/"
  }
}
