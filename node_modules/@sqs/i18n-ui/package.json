  "jest": {
    "testMatch": [
      "**/tests/**/?(*.)unit.ts?(x)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "transform": {
      "^.+.jsx?$": "babel-jest",
      "^.+.tsx?$": [
        "ts-jest",
        {
          "packageJson": "package.json",
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:lib",
    "build:lib": "tsc && babel --verbose ./src --out-dir lib",
    "clean": "rimraf lib",
    "lint:fix": "eslint --max-warnings=0 --ext .ts,.tsx,.js --fix src tests",
    "lint": "eslint --max-warnings=0 --ext .ts,.tsx,.js src tests",
    "test": "jest",
    "type-check": "tsc",
    "watch": "npm run build:lib -- --watch"
  }
