    "generate:images": "node ./bin/generate-images/index.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0 src/",
    "start": "febs start",
    "serve:ci": "serve --single dist --listen 5000",
    "test": "jest --passWithNoTests",
    "watch": "webpack --watch"
  },
