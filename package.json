{
  "name": "@sqs/creative-sandbox",
  "version": "1.0.1",
  "description": "A sandbox environment for the creative team developers for usage within www",
  "author": "frontsite",
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "config": {
    "destination_dir": "dist"
  },
  "types": "dist/index.d.ts",
